#var {prompt_data} {}

#prompt {^%+>$} {@prompt{%0}}

#function {prompt} {
  #var {prompt_data} {%0};
  
  #if {"$battle" == "on"} {
    #if {"$pk" == "on"} {
      #return <019>spd<099> <059>pk<099> $prompt_data
    } {
      #return <019>spd<099> $prompt_data
    }
  } {
    #if {"$pk" == "on"} {
      #return <059>pk<099> $prompt_data
    } {
      #return $prompt_data
    }
  }
}

#nop Gag blank lines.
#gag {^{(?:[\s]*)}$}
